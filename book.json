{
    "gitbook": "3.x.x",
    "title": "Learn Lua With Me",
    "description": "A Guide to Lua with code examples",
    "author": "NCSham",
    "language": "en",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "include-codeblock",
        "ace",
        "highlight",
        "search"
